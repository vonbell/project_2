<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Finsta</title>
</head>

<body>
    <%- include('./partials/header') %>
    <ul>
    <% posts.forEach(post=> { %>
        <li id="full-post">

            <nav class="post-header-bar">
                <div id="avi-username">
                    <a href="/<%=post._id; %>"><img src="<%=post.avi; %>" alt="<%= post.name %>" height="32" width="32" style="border-radius: 50%;" id="avi"></a>
                    <a href="/<%=post._id; %>" class="username"><%= post.name %></a>
                </div>

                <div id="edit-delete">
                    <a href="/<%= post._id %>/edit"><img src="data:image/png;base64," alt="Edit pic" height="28" width="28" id="edit-pic"></a>
                    <form action="/<%=post.id%>?_method=DELETE" method="POST" style="display:inline;">
                        <input type="image" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRRBXaR_x4cwDv5COtKyccXX1ZxwV8oYDHPYNLi2BAC_YcgroTYfZjs1aXfq2Pe-zS_D0E&usqp=CAU" alt="Submit" height="28" width="28" id="delete-pic" />
                    </form>
                </div>
            </nav>
            
            <div>
                <img src="<%= post.img %>" alt="Finsta post" height="614" width="614" id="post-img">
            </div>

            <div id="caption">
                <p><a href="/<%=post._id; %>" class="username"><%= post.name %></a></p>
                <p><%= post.caption %></p>
            </div>
            
        </li>
    <% }); %>
    </ul>
</body>

</html>